                <Button x:Name="btn1" Content="Импорт данных" FontSize="20" Margin="0 10" Click="btn1_Click"/>
                <Button x:Name="btn2" Content="Экспорт в EXCEL" FontSize="20" Margin="0 10" Click="btn2_Click"/>
                <Button x:Name="btn3" Content="Импорт JSON данных" FontSize="20" Margin="0 10" Click="btn3_Click"/>
                <Button x:Name="btn4" Content="Экспорт в WORD" FontSize="20" Margin="0 10" Click="btn4_Click"/>
            </StackPanel>
        </StackPanel>
        
    </Grid>
